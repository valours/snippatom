#snippets for typescript
'.source.ts':
  # helloworld
  'Hello World':
    'prefix':'test'
    'body':"""
    Hello World
    """
  # switch case
  'switch/case':
    'prefix': 'sc'
    'body':"""
    switch (${1}) {
      case ${2}: ${3}{
        ${4};
        break;
      }
      default: {
        break;
      }
    }
    """

    # private method
    'private method':
      'prefix': 'prm'
      'body':"""
      private ${1}(${2}): ${3} {
        ${4}
      }
      """
